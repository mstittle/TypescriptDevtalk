{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "outDir": "out",
        "strictNullChecks": true,
        "jsx" : "react"
    },
    "exclude": [
        "node_modules"
    ],
    "include" : [
        "src/*.ts",
        "src/*.tsx"
    ]
}